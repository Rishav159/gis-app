{
  "extends": "dhis2",
  "rules": {
    "space-after-keywords": "off",
    "space-return-throw-case": "off",
    "consistent-return": "off",
    "prefer-const": "off",
    "func-names": 0,
    "one-var": 0,
    "no-var": 0,
    "vars-on-top": 0,
    "comma-dangle": 0,
    "dot-notation": 0,
    "no-shadow": 0,
    "new-cap": 0,
  }
}